{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "ordersByStatus": {
      "path": "/_v/order_monitor/:status",
      "public": true
    },
    "totalOrdersByStatus": {
      "path": "/_v/order_monitor/totalOrders/:status",
      "public": true
    },
    "notificationByStatus": {
      "path": "/_v/order_monitor/getNotification/:status",
      "public": true
    },
    "setNotification": {
      "path": "/_v/order_monitor/setNotification/:status/:number",
      "public": true
    },
    "emailsByStatus": {
      "path": "/_v/order_monitor/getEmails/:status",
      "public": true
    },
    "setEmails": {
      "path": "/_v/order_monitor/setEmails/:status/:emails",
      "public": true
    }
  }
}